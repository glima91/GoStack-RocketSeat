outes from './routes';\nimport GlobalStyle from './styles/global';\nimport SignIn from './pages/SignIn';\n//import SignUp from './pages/SignUp';\n\nimport AppProvider from './hooks';\n\nconst App: React.FC = () =>(\n  <Router>\n   <AppProvider>\n      <Routes />\n   </AppProvider>  \n    <GlobalStyle />\n  </Router>\n);\n\nexport default App;\n","/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/styles/global.ts",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/pages/SignIn/index.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/pages/SignIn/styles.ts",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/components/Button/index.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/components/Input/index.tsx",["111"],"import { useField } from '@unform/core';\nimport React, { InputHTMLAttributes, useEffect, useRef, useState, useCallback } from 'react';\nimport { IconBaseProps } from 'react-icons';\nimport { FiAlertCircle } from 'react-icons/fi';\n\nimport { Container, Error } from './styles';\nimport Tooltip from '../Tooltip';\n\ninterface InputProps extends InputHTMLAttributes<HTMLInputElement> {\n  name: string;\n  icon?: React.ComponentType<IconBaseProps>;\n}\n\nconst Input: React.FC<InputProps> = ({ name, icon: Icon, ...rest }) => {\n  const inputRef = useRef<HTMLInputElement>(null);\n\n  const [isFocused, setIsFocused] = useState(false);\n  const [isFilled, setIsFilled] = useState(false);\n\n  const { fieldName, defaultValue, error, registerField } = useField(name);\n  \n  const handleInputFocus = useCallback(() => {\n    setIsFocused(true);\n  }, []);\n\n\nconst handleInputBlur = useCallback(() => {\n  setIsFocused(false);\n\n  setIsFilled(!!inputRef.current?.value);\n\n}, []);\n\n\n  useEffect(() => {\n    registerField({\n      name: fieldName,\n      ref: inputRef.current,\n      path: 'value',\n    });\n  }, [fieldName, registerField]);\n\n\n  return (\n    <Container isErrored={!!error} isFilled={isFilled} isFocused={isFocused}>\n    {Icon && <Icon size={20}/>}\n    <input  \n      onFocus={handleInputFocus}\n      onBlur={handleInputBlur}\n      defaultValue={defaultValue} \n      ref={inputRef} \n      {...rest}/>\n\n      {error && <Error title={error}>\n                  <FiAlertCircle color=\"#c53030\" size={20}/>\n                </Error>}\n\n    </Container>\n  );\n};\n\nexport default Input;","/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/components/Input/styles.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/components/Button/styles.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/pages/SignUp/index.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/pages/SignUp/styles.ts",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/utils/getValidationErrors.ts",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/components/Tooltip/index.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/components/Tooltip/styles.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/service/api.ts",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/hooks/auth.tsx",["112"],"import userEvent from '@testing-library/user-event';\nimport React, { createContext, useCallback, useState, useContext } from 'react';\nimport api from '../service/api';\n\ninterface AuthState {\n  token: string;\n  user: object;\n}\n\ninterface SignInCredentials {\n  email: string;\n  password: string;\n}\n\ninterface AuthContextData {\n  user: object;\n  signIn(credentials: SignInCredentials): Promise<void>;\n  signOut(): void;\n}\n\nconst AuthContext = createContext<AuthContextData>({} as AuthContextData);\n\nexport const AuthProvider: React.FC = ({ children }) => {\n\n  const [data, setData] = useState<AuthState>(() => {\n    const token = localStorage.getItem('@Gobarber:token');\n    const user = localStorage.getItem('@Gobarber:user');\n\n    if (token && user) {\n      return { token, user: JSON.parse(user) };\n    }\n\n    return {} as AuthState;\n  });\n\n  const signIn = useCallback(async ({ email , password }) => {\n    const response = await api.post('sessions', {\n      email,\n      password\n    });\n\n    const { token, user } = response.data;\n\n    localStorage.setItem('@Gobarber:token', token);\n    localStorage.setItem('@Gobarber:user', JSON.stringify(user));\n\n    setData({ token, user });  \n  }, []);\n\n  const signOut = useCallback(() => {\n    localStorage.removeItem('@Gobarber:token');\n    localStorage.removeItem('@Gobarber:user');\n\n    setData({} as AuthState);\n  }, []);\n\nreturn (\n  <AuthContext.Provider value={{ user: data.user , signIn, signOut  }}>\n    { children }\n  </AuthContext.Provider>\n  );\n}\n\nexport function useAuth(): AuthContextData {\n  const context = useContext(AuthContext);\n\n  if (!context) {\n    throw new Error('useAuth must be used within a AuthProvider');\n  }\n  return context;\n}","/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/hooks/index.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/hooks/toast.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/components/ToastContainer/index.tsx",["113"],"import React from 'react';\nimport { useTransition } from 'react-spring';\n\nimport { Container } from './styles';\nimport Toast from './Toast';\nimport { ToastMessage, useToast } from '../../hooks/toast';\n\ninterface ToastContainerProps {\n  messages: ToastMessage[];\n}\n\n\nconst ToastContainer: React.FC<ToastContainerProps> = ( {messages} ) => {\n  const  messageWithTransitions = useTransition(\n    messages,\n    message => message.id,\n    {\n      from: { right: '-120%', opacity: 0 },\n      enter: { right: '0%', opacity: 1 },\n      leave: { right: '-120%', opacity: 0 },\n    }\n  );\n\n\n  return(\n  <Container>\n    {messageWithTransitions.map(({ item, key, props }) => (\n      <Toast key={key} style={props} message={item}/>\n        \n    ))}  \n  </Container>\n  );\n};\n\nexport default ToastContainer;","/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/components/ToastContainer/styles.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/components/ToastContainer/Toast/index.tsx",["114"],"import React, { useEffect } from 'react';\n\n\nimport { FiAlertCircle, FiXCircle, FiInfo, FiCheckCircle } from 'react-icons/fi';\nimport { Container } from './styles';\n\nimport { ToastMessage, useToast } from '../../../hooks/toast';\nimport { ObjectType } from 'typescript';\n\ninterface ToastProps {\n  message: ToastMessage;\n  style: Object;\n}\n\nconst icons = {\n  info: <FiInfo size={24} />,\n  error: <FiAlertCircle size={24} />,\n  sucess: <FiCheckCircle size={24} />,\n};\n\nconst Toast: React.FC<ToastProps> = ({ message, style }) => {\n  const { removeToast } = useToast();\n\n  useEffect(() => {\n    const timer = setTimeout(() => {\n      removeToast(message.id);\n    }, 3000);\n\n    return () => {\n      clearTimeout(timer);\n    };\n  }, [removeToast, message.id]);\n\n  return (\n  <Container type={message.type} hasDescription={!!message.description} style={style}>\n  {icons[message.type || 'info']}\n  <div>\n    <strong>{message.title}</strong>\n      {message.description && <p>{message.description}</p>}\n  </div>\n  <button onClick={() => removeToast(message.id)} type=\"button\">\n    <FiXCircle size={18} />\n  </button>\n  </Container>\n  );\n};\n\n\nexport default Toast;","/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/components/ToastContainer/Toast/styles.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/routes/index.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/src/pages/Dashboard/index.tsx",[],"/home/gustavolima/Projetos/GoStack-RocketSeat/07-gobarber-web/sr